{
  "expo": {
    "name": "My To-do List",
    "slug": "todo-list",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#E0E0E0"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#E0E0E0"
      },
      "package": "com.richiecodes.todolist"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "alpha",
    "githubUrl": "https://github.com/richiecodes/Todo-List-App"
  }
}
